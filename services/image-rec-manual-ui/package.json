    "regenerator-runtime": "^0.13.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.86.0",
    "socket.io-client": "^2.2.0",
    "superagent": "^4.1.0",
    "superagent-protobuf": "^0.1.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.5.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.3",
    "eslint-plugin-babel": "^5.3.0",
